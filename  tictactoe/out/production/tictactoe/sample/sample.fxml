<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="0" vgap="0"
          fx:id="pane"
          stylesheets="sample/Styles.css">
    <padding>
        <Insets topRightBottomLeft="15" />
    </padding>
    <columnConstraints>
        <ColumnConstraints prefWidth="40" />
        <ColumnConstraints prefWidth="40" />
        <ColumnConstraints prefWidth="40" />
        <ColumnConstraints prefWidth="40" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints prefHeight="40"  />
        <RowConstraints prefHeight="40"  />
        <RowConstraints prefHeight="40"  />
        <RowConstraints prefHeight="40"  />
    </rowConstraints>

    <Label GridPane.RowIndex="0" GridPane.columnIndex="0" text="" styleClass="top-left-label"
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="0" GridPane.columnIndex="1" text="" styleClass="top-label"
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="0" GridPane.columnIndex="2" text="" styleClass="top-label"
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="0" GridPane.columnIndex="3" text="" styleClass="top-label"
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />

    <Label GridPane.RowIndex="1" GridPane.columnIndex="0" text="" styleClass="left-label"
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="1" GridPane.columnIndex="1" text=""
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="1" GridPane.columnIndex="2" text=""
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="1" GridPane.columnIndex="3" text=""
           GridPane.fillWidth="true" maxWidth="Infinity"
           GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />

    <Label GridPane.RowIndex="2" GridPane.columnIndex="0" text="" styleClass="left-label"
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="2" GridPane.columnIndex="1" text=""
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="2" GridPane.columnIndex="2" text=""
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="2" GridPane.columnIndex="3" text=""
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />

    <Label GridPane.RowIndex="3" GridPane.columnIndex="0" text="" styleClass="left-label"
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="3" GridPane.columnIndex="1" text=""
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="3" GridPane.columnIndex="2" text=""
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />
    <Label GridPane.RowIndex="3" GridPane.columnIndex="3" text="" 
            GridPane.fillWidth="true" maxWidth="Infinity"
            GridPane.fillHeight="true" maxHeight="Infinity"
           onMouseClicked="#handleClick" alignment="CENTER"
    />

</GridPane>